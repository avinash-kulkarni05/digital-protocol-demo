      {
        "field": "cdashDomain",
        "description": "CDISC CDASH domain code (LB, VS, EG, PE, AE, CM, etc.)"
      },
      {
        "field": "cdiscCode",
        "description": "NCI Thesaurus concept code (e.g., C78713 for Complete Blood Count)"
      },
      {
        "field": "cdiscDecode",
        "description": "Human-readable decode for the CDISC code"
      },
      {
        "field": "category",
        "description": "High-level categorization (LABORATORY, VITAL_SIGNS, ECG, etc.)"
      },
      {
        "field": "confidence",
        "description": "Model confidence score (0.0-1.0) for the mapping"
      }
    ],
