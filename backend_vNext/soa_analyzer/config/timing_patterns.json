  "patterns": {
    "bi_eoi": {
      "id": "bi_eoi",
      "pattern_regex": "^(BI|EOI|SOI)[/,&\\s]+(and\\s+)?(EOI|BI|SOI)$",
      "atomic_timings": ["BI", "EOI"],
      "description": "Before Infusion / End of Infusion split for PK sampling"
    },
    "pre_post_dose": {
      "id": "pre_post_dose",
      "pattern_regex": "^pre[- ]?dose[/,&\\s]+(and\\s+)?post[- ]?dose$",
      "atomic_timings": ["pre-dose", "post-dose"],
      "description": "Pre-dose / Post-dose split for PK sampling"
    },
    "trough_peak": {
      "id": "trough_peak",
      "pattern_regex": "^(trough|peak)[/,&\\s]+(and\\s+)?(peak|trough)$",
      "atomic_timings": ["trough", "peak"],
      "description": "Trough/Peak PK sampling split"
    },
    "fasting_fed": {
      "id": "fasting_fed",
      "pattern_regex": "^(fasting|fed)[/,&\\s]+(and\\s+)?(fed|fasting)$",
      "atomic_timings": ["fasting", "fed"],
      "description": "Fasting/Fed condition split for food effect studies"
    },
    "hour_offsets": {
      "id": "hour_offsets",
      "pattern_regex": "^(\\d+h?[,\\s]+)+\\d+h?\\s*(post[- ]?dose|pre[- ]?dose)?$",
      "atomic_timings": [],
      "description": "Multiple hour offsets (e.g., '0h, 2h, 4h post-dose') - dynamic expansion"
    },
    "minute_offsets": {
      "id": "minute_offsets",
      "pattern_regex": "^(\\d+\\s*min[,\\s]+)+\\d+\\s*min\\s*(pre[- ]?dose|post[- ]?dose)?$",
      "atomic_timings": [],
      "description": "Multiple minute offsets (e.g., '30min, 60min pre-dose') - dynamic expansion"
    },
    "day_offsets": {
      "id": "day_offsets",
      "pattern_regex": "^(Days?\\s*\\d+[,&\\s]+(and\\s+)?)+Days?\\s*\\d+$",
      "atomic_timings": [],
      "description": "Multiple day offsets (e.g., 'Day 1, Day 8, Day 15') - dynamic expansion"
    },
    "week_offsets": {
      "id": "week_offsets",
      "pattern_regex": "^(Weeks?\\s*\\d+[,&\\s]+(and\\s+)?)+Weeks?\\s*\\d+$",
      "atomic_timings": [],
      "description": "Multiple week offsets (e.g., 'Week 1, Week 4, Week 8') - dynamic expansion"
    },
    "cycle_day": {
      "id": "cycle_day",
      "pattern_regex": "^(C\\d+D\\d+[,&\\s]+(and\\s+)?)+C\\d+D\\d+$",
      "atomic_timings": [],
      "description": "Cycle-Day notation (e.g., 'C1D1, C2D1') - dynamic expansion"
    },
    "cycle_long": {
      "id": "cycle_long",
      "pattern_regex": "^(Cycle\\s+\\d+\\s+Day\\s+\\d+[,&\\s]+(and\\s+)?)+Cycle\\s+\\d+\\s+Day\\s+\\d+$",
      "atomic_timings": [],
      "description": "Long cycle-day notation (e.g., 'Cycle 1 Day 1, Cycle 2 Day 1') - dynamic expansion"
    }
  },
